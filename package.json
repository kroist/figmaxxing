{
  "name": "figmaxxing",
  "version": "0.1.3",
  "description": "Capture any EVM DApp's UI into Figma with an injected wallet, powered by a terminal interface",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "figmaxxing": "./bin/figmaxxing.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.tsx",
    "postinstall": "chmod +x node_modules/node-pty/prebuilds/*/spawn-helper 2>/dev/null || true",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "dependencies": {
    "ink": "^6.8.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "node-pty": "^1.1.0",
    "playwright": "^1.58.2",
    "react": "^19.2.4",
    "viem": "^2.46.2"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
